  <select id="selectSysElementGroupSysElement" resultMap="SysElementGroupVoMap" parameterType="com.ray.demo.admin.model.master.SysElementGroup" >
select b.ELE_GROUP_NAME,count(a.ELE_GROUP_ID) AS StypeCount,a.ELE_GROUP_ID,b.id,b.CRT_USER,b.ELE_GROUP_CODE   from
sys_element AS a 
 RIGHT   join sys_element_group AS b ON a.ELE_GROUP_ID=b.id 
 
 where 
  b.STATUS_FLAG=0 AND ((a.STATUS_FLAG is null) OR (a.STATUS_FLAG=0) OR (a.STATUS_FLAG=2))  
 
  <if test="eleGroupName != null">
        and
        b.ELE_GROUP_NAME like CONCAT('%',#{eleGroupName},'%')
      </if>
